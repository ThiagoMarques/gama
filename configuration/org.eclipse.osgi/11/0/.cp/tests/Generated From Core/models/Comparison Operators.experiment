experiment "Run Tests" type: test {	

	
		test "between" {
			bool var2 <- (date('2016-01-01') between(date('2000-01-01'), date('2020-02-02'))); 	// var2 equals true
			assert var2 = true; 
			//// will return true every new day between these two dates, taking the first one as the starting point
			//every(#day between(date('2000-01-01'), date('2020-02-02'))) 
			bool var5 <- (date('2016-01-01') between(date('2000-01-01'), date('2020-02-02'))); 	// var5 equals true
			assert var5 = true; 
			//// // will return true if the current_date of the model is in_between the 2
			//between(date('2000-01-01'), date('2020-02-02'))
			bool var0 <- between(5.0, 1.0, 10.0); 	// var0 equals true
			assert var0 = true; 
			bool var1 <- between(5, 1, 10); 	// var1 equals true
			assert var1 = true; 

		}
	
		test "Different" {
			bool var0 <- [2,3] != [2,3]; 	// var0 equals false
			assert var0 = false; 
			bool var1 <- [2,4] != [2,3]; 	// var1 equals true
			assert var1 = true; 
			bool var2 <- 3.0 != 3; 	// var2 equals false
			assert var2 = false; 
			bool var3 <- 4.7 != 4; 	// var3 equals true
			assert var3 = true; 
			bool var4 <- 3.0 != 3.0; 	// var4 equals false
			assert var4 = false; 
			bool var5 <- 4.0 != 4.7; 	// var5 equals true
			assert var5 = true; 
			bool var6 <- 3 != 3.0; 	// var6 equals false
			assert var6 = false; 
			bool var7 <- 4 != 4.7; 	// var7 equals true
			assert var7 = true; 
			bool var8 <- #now != #now minus_hours 1; 	// var8 equals true
			assert var8 = true; 

		}
	
		test "Equals" {
			bool var0 <- [2,3] = [2,3]; 	// var0 equals true
			assert var0 = true; 
			bool var1 <- 4 = 5; 	// var1 equals false
			assert var1 = false; 
			bool var2 <- #now = #now minus_hours 1; 	// var2 equals false
			assert var2 = false; 
			bool var3 <- 4.7 = 4; 	// var3 equals false
			assert var3 = false; 
			bool var4 <- 4.5 = 4.7; 	// var4 equals false
			assert var4 = false; 
			bool var5 <- 3 = 3.0; 	// var5 equals true
			assert var5 = true; 
			bool var6 <- 4 = 4.7; 	// var6 equals false
			assert var6 = false; 

		}
	
		test "GE" {
			bool var0 <- {5,7} >= {4,6}; 	// var0 equals true
			assert var0 = true; 
			bool var1 <- {5,7} >= {4,8}; 	// var1 equals false
			assert var1 = false; 
			bool var2 <- 'abc' >= 'aeb'; 	// var2 equals false
			assert var2 = false; 
			bool var3 <- 'abc' >= 'abc'; 	// var3 equals true
			assert var3 = true; 
			bool var4 <- 3.5 >= 7; 	// var4 equals false
			assert var4 = false; 
			bool var5 <- 3.5 >= 3.5; 	// var5 equals true
			assert var5 = true; 
			bool var6 <- 3 >= 7; 	// var6 equals false
			assert var6 = false; 
			bool var7 <- 3 >= 2.5; 	// var7 equals true
			assert var7 = true; 
			bool var8 <- #now >= #now minus_hours 1; 	// var8 equals true
			assert var8 = true; 

		}
	
		test "GT" {
			bool var5 <- 'abc' > 'aeb'; 	// var5 equals false
			assert var5 = false; 
			bool var6 <- {5,7} > {4,6}; 	// var6 equals true
			assert var6 = true; 
			bool var7 <- {5,7} > {4,8}; 	// var7 equals false
			assert var7 = false; 
			bool var0 <- 3.5 > 7.6; 	// var0 equals false
			assert var0 = false; 
			bool var1 <- 3.5 > 7; 	// var1 equals false
			assert var1 = false; 
			bool var2 <- 3 > 2.5; 	// var2 equals true
			assert var2 = true; 
			bool var3 <- (#now > (#now minus_hours 1)); 	// var3 equals true
			assert var3 = true; 
			bool var4 <- 13.0 > 7.0; 	// var4 equals true
			assert var4 = true; 

		}
	
		test "LE" {
			bool var0 <- 'abc' <= 'aeb'; 	// var0 equals true
			assert var0 = true; 
			bool var1 <- {5,7} <= {4,6}; 	// var1 equals false
			assert var1 = false; 
			bool var2 <- {5,7} <= {4,8}; 	// var2 equals false
			assert var2 = false; 
			bool var3 <- 3 <= 7; 	// var3 equals true
			assert var3 = true; 
			bool var4 <- (#now <= (#now minus_hours 1)); 	// var4 equals false
			assert var4 = false; 
			bool var5 <- 3 <= 2.5; 	// var5 equals false
			assert var5 = false; 
			bool var6 <- 3.5 <= 3.5; 	// var6 equals true
			assert var6 = true; 
			bool var7 <- 7.0 <= 7; 	// var7 equals true
			assert var7 = true; 

		}
	
		test "LT" {
			bool var5 <- {5,7} < {4,6}; 	// var5 equals false
			assert var5 = false; 
			bool var6 <- {5,7} < {4,8}; 	// var6 equals false
			assert var6 = false; 
			bool var7 <- 'abc' < 'aeb'; 	// var7 equals true
			assert var7 = true; 
			bool var0 <- #now < #now minus_hours 1; 	// var0 equals false
			assert var0 = false; 
			bool var1 <- 3 < 2.5; 	// var1 equals false
			assert var1 = false; 
			bool var2 <- 3.5 < 7.6; 	// var2 equals true
			assert var2 = true; 
			bool var3 <- 3 < 7; 	// var3 equals true
			assert var3 = true; 
			bool var4 <- 3.5 < 7; 	// var4 equals true
			assert var4 = true; 

		}
	
}

	